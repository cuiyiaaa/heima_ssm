<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.ssm.mapper.SysLogMapper">


    <select id="findSysLogAll" resultType="SysLog">
        SELECT * FROM syslog
    </select>

    <!--保存日志信息-->
    <insert id="saveSysLog" parameterType="com.itheima.ssm.domain.SysLog">
       insert into syslog(visitTime,username,ip,url,executionTime,method) values(#{visitTime},#{username},#{ip},#{url},#{executionTime},#{method})
   </insert>
</mapper>